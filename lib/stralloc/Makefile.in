# $Id: Makefile.in,v 1.3 2005/04/15 18:15:12 smoli Exp $
# ===========================================================================
#
# Makefile for lib/scan subdirectory
#
# relative directories
# ---------------------------------------------------------------------------
srcdir       = @srcdir@
top_srcdir   = @top_srcdir@
top_builddir = @top_builddir@
thisdir      = lib/stralloc/

VPATH        = $(srcdir)

# include global make configuration
# ---------------------------------------------------------------------------
include $(top_builddir)/config.mk

# configuration for this directory
# ---------------------------------------------------------------------------
LIB = ../libshish.a

@LIBOWFAT_FALSE@MODULES += stralloc_cat
@LIBOWFAT_FALSE@MODULES += stralloc_catb
MODULES += stralloc_catc
@LIBOWFAT_FALSE@MODULES += stralloc_catlong0
@LIBOWFAT_FALSE@MODULES += stralloc_cats
@LIBOWFAT_FALSE@MODULES += stralloc_catulong0
@LIBOWFAT_FALSE@MODULES += stralloc_copy
@LIBOWFAT_FALSE@MODULES += stralloc_copyb
@LIBOWFAT_FALSE@MODULES += stralloc_copys
@LIBOWFAT_FALSE@MODULES += stralloc_diffs
@LIBOWFAT_FALSE@@DEBUG_FALSE@MODULES += stralloc_free
@DEBUG_TRUE@MODULES += stralloc_freedebug
@LIBOWFAT_FALSE@MODULES += stralloc_init
MODULES += stralloc_insertb
MODULES += stralloc_move
MODULES += stralloc_nul
@DEBUG_FALSE@MODULES += stralloc_ready
@DEBUG_TRUE@MODULES += stralloc_readydebug
@DEBUG_FALSE@MODULES += stralloc_readyplus
@DEBUG_TRUE@MODULES += stralloc_readyplusdebug
MODULES += stralloc_remove
@DEBUG_FALSE@MODULES += stralloc_trunc
@DEBUG_TRUE@MODULES += stralloc_truncdebug
MODULES += stralloc_write
@LIBOWFAT_FALSE@MODULES += stralloc_zero


SOURCES = $(MODULES:%=%.c)
OBJECTS = $(MODULES:%=%.o)
DEPS    = $(MODULES:%=%.d)

# targets for this directory
# ---------------------------------------------------------------------------
@DEPS_TRUE@all: Makefile .libdep
@DEPS_FALSE@all: Makefile $(LIB)
install: 

# dependencies
# ---------------------------------------------------------------------------
-include $(DEPS)

# local build configuration
# ---------------------------------------------------------------------------
INCLUDES += -I$(top_srcdir)/lib -I.
#INCLUDES += -I$(top_builddir)/lib

# include build.mk (makefile helper)
# ---------------------------------------------------------------------------
include $(top_builddir)/build.mk
