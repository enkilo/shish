# $Id: Makefile.in,v 1.5 2005/04/15 18:15:12 smoli Exp $
# ===========================================================================
#
# Makefile for lib/byte subdirectory
#
# relative directories
# ---------------------------------------------------------------------------
srcdir       = @srcdir@
top_srcdir   = @top_srcdir@
top_builddir = @top_builddir@
thisdir      = lib/byte/

VPATH        = $(srcdir)

# include global make configuration
# ---------------------------------------------------------------------------
include $(top_builddir)/config.mk

# configuration for this directory
# ---------------------------------------------------------------------------
LIB = ../libshish.a

@LIBOWFAT_FALSE@MODULES += byte_chr
@LIBOWFAT_FALSE@MODULES += byte_copy
@LIBOWFAT_FALSE@MODULES += byte_copyr
@LIBOWFAT_FALSE@MODULES += byte_diff
MODULES += byte_fill
@LIBOWFAT_FALSE@MODULES += byte_rchr
@LIBOWFAT_FALSE@MODULES += byte_zero

          
SOURCES = $(MODULES:%=%.c)
OBJECTS = $(MODULES:%=%.o)
DEPS    = $(MODULES:%=%.d)

# targets for this directory
# ---------------------------------------------------------------------------
@DEPS_TRUE@all: Makefile .libdep
@DEPS_FALSE@all: Makefile $(LIB)
install: 

# dependencies
# ---------------------------------------------------------------------------
-include $(DEPS)

# local build configuration
# ---------------------------------------------------------------------------
INCLUDES += -I$(top_srcdir)/lib -I.
#INCLUDES += -I$(top_builddir)/lib

# include build.mk (makefile helper)
# ---------------------------------------------------------------------------
include $(top_builddir)/build.mk
