# $Id: Makefile.in,v 1.10 2005/04/15 18:15:12 smoli Exp $
# ===========================================================================
#
# Makefile for lib/buffer subdirectory
#
# relative directories
# ---------------------------------------------------------------------------
srcdir       = @srcdir@
top_srcdir   = @top_srcdir@
top_builddir = @top_builddir@
thisdir      = lib/buffer/

VPATH        = $(srcdir)

# include global make configuration
# ---------------------------------------------------------------------------
include $(top_builddir)/config.mk
          
# configuration for this directory
# ---------------------------------------------------------------------------
LIB = ../libshish.a

@LIBOWFAT_FALSE@MODULES += buffer_close
MODULES += buffer_default
MODULES += buffer_dump
MODULES += buffer_dummyread
MODULES += buffer_dummyreadmmap
@LIBOWFAT_FALSE@MODULES += buffer_feed
@LIBOWFAT_FALSE@MODULES += buffer_flush
@LIBOWFAT_FALSE@MODULES += buffer_free
@LIBOWFAT_FALSE@MODULES += buffer_munmap
@LIBOWFAT_FALSE@MODULES += buffer_fromsa
@LIBOWFAT_FALSE@MODULES += buffer_frombuf
MODULES += buffer_fromstr
@LIBOWFAT_FALSE@MODULES += buffer_get
MODULES += buffer_get_until
@LIBOWFAT_FALSE@MODULES += buffer_getc
@LIBOWFAT_FALSE@MODULES += buffer_init
MODULES += buffer_mmapprivate
@LIBOWFAT_FALSE@MODULES += buffer_mmapread
MODULES += buffer_mmapread_fd
MODULES += buffer_prefetch
@LIBOWFAT_FALSE@MODULES += buffer_put
MODULES += buffer_putc
@LIBOWFAT_FALSE@MODULES += buffer_putflush
@LIBOWFAT_FALSE@MODULES += buffer_putm_internal
@LIBOWFAT_FALSE@MODULES += buffer_putnlflush
MODULES += buffer_putnspace
@LIBOWFAT_FALSE@MODULES += buffer_puts
@LIBOWFAT_FALSE@MODULES += buffer_putsa
@LIBOWFAT_FALSE@MODULES += buffer_putsflush
@LIBOWFAT_FALSE@MODULES += buffer_putspace
@LIBOWFAT_FALSE@MODULES += buffer_putulong
MODULES += buffer_skip_until
@LIBOWFAT_FALSE@MODULES += buffer_stubborn
@LIBOWFAT_FALSE@MODULES += buffer_stubborn2
@LIBOWFAT_FALSE@MODULES += buffer_tosa
MODULES += buffer_truncfile

          
SOURCES = $(MODULES:%=%.c)
OBJECTS = $(MODULES:%=%.o)
DEPS    = $(MODULES:%=%.d)

# targets for this directory
# ---------------------------------------------------------------------------
all: Makefile @DEPS_TRUE@.libdep
install: 

# dependencies
# ---------------------------------------------------------------------------
-include $(DEPS)

# local build configuration
# ---------------------------------------------------------------------------
INCLUDES += -I$(top_srcdir)/lib -I.
#INCLUDES += -I$(top_builddir)/lib
          
# include build.mk (makefile helper)
# ---------------------------------------------------------------------------
include $(top_builddir)/build.mk
