# $Id: Makefile.in,v 1.3 2005/04/15 18:15:12 smoli Exp $
# ===========================================================================
#
# Makefile for lib/scan subdirectory
#
# relative directories
# ---------------------------------------------------------------------------
srcdir       = @srcdir@
top_srcdir   = @top_srcdir@
top_builddir = @top_builddir@
thisdir      = lib/scan/

VPATH        = $(srcdir)

# include global make configuration
# ---------------------------------------------------------------------------
include $(top_builddir)/config.mk

# configuration for this directory
# ---------------------------------------------------------------------------
LIB = ../libshish.a
@LIBOWFAT_FALSE@MODULES += scan_8long
@LIBOWFAT_FALSE@MODULES += scan_int
@LIBOWFAT_FALSE@MODULES += scan_long
@LIBOWFAT_FALSE@MODULES += scan_uint
@LIBOWFAT_FALSE@MODULES += scan_ulong
@LIBOWFAT_FALSE@MODULES += scan_8longn scan_longn scan_ulongn



SOURCES = $(MODULES:%=%.c)
OBJECTS = $(MODULES:%=%.o)
DEPS    = $(MODULES:%=%.d)

# targets for this directory
# ---------------------------------------------------------------------------
@DEPS_TRUE@all: Makefile .libdep
@DEPS_FALSE@all: Makefile $(LIB)
install: 

# dependencies
# ---------------------------------------------------------------------------
-include $(DEPS)

# local build configuration
# ---------------------------------------------------------------------------
INCLUDES += -I$(top_srcdir)/lib -I.
INCLUDES += -I$(top_builddir)

# include build.mk (makefile helper)
# ---------------------------------------------------------------------------
include $(top_builddir)/build.mk
