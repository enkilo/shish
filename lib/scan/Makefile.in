# $Id: Makefile.in,v 1.3 2005/04/15 18:15:12 smoli Exp $
# ===========================================================================
#
# Makefile for lib/scan subdirectory
#
# relative directories
# ---------------------------------------------------------------------------
srcdir       = @srcdir@
top_srcdir   = @top_srcdir@
top_builddir = @top_builddir@
thisdir      = lib/scan/

VPATH        = $(srcdir)

# include global make configuration
# ---------------------------------------------------------------------------
include $(top_builddir)/config.mk

# configuration for this directory
# ---------------------------------------------------------------------------
LIB = ../libshish.a
MODULES = scan_8int scan_8long scan_8longn scan_8short scan_asn1derlength scan_asn1dertag scan_charsetnskip scan_double scan_eolskip scan_fromhex scan_httpdate scan_int scan_line scan_lineskip scan_long scan_longlong scan_longn scan_netstring scan_noncharsetnskip scan_nonwhitenskip scan_octal scan_pb_tag scan_pb_type0_sint scan_pb_type1_fixed64 scan_pb_type5_fixed32 scan_plusminus scan_short scan_uint scan_ulong scan_ulonglong scan_ulongn scan_ushort scan_utf8 scan_utf8_sem scan_varint scan_whitenskip scan_xchar scan_xint scan_xlong scan_xlonglong scan_xlongn scan_xmlescape scan_xshort

SOURCES = $(MODULES:%=%.c)
OBJECTS = $(MODULES:%=%.o)
DEPS    = $(MODULES:%=%.d)

# targets for this directory
# ---------------------------------------------------------------------------
all: Makefile .libdep
install: 

# dependencies
# ---------------------------------------------------------------------------
-include $(DEPS)

# local build configuration
# ---------------------------------------------------------------------------
INCLUDES += -I$(top_srcdir)/lib -I.
INCLUDES += -I$(top_builddir)

# include build.mk (makefile helper)
# ---------------------------------------------------------------------------
include $(top_builddir)/build.mk
