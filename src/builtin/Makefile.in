# $Id: Makefile.in,v 1.8 2005/04/23 10:19:32 smoli Exp $
# ===========================================================================
#
# Makefile for src/builtin subdirectory
#
# relative directories
# ---------------------------------------------------------------------------
srcdir       = @srcdir@
top_srcdir   = @top_srcdir@
top_builddir = @top_builddir@
thisdir      = src/builtin/

VPATH        = $(srcdir)

# include global make configuration
# ---------------------------------------------------------------------------
include $(top_builddir)/config.mk

# configuration for this directory
# ---------------------------------------------------------------------------
ARCHIVE = builtin.a
MODULES += builtin_basename
MODULES += builtin_break
MODULES += builtin_cd
MODULES += builtin_dirname
MODULES += builtin_dump
MODULES += builtin_echo
MODULES += builtin_error
MODULES += builtin_eval
MODULES += builtin_exec
MODULES += builtin_exit
MODULES += builtin_export
MODULES += builtin_false
MODULES += builtin_fdtable
MODULES += builtin_hash
MODULES += builtin_history
MODULES += builtin_hostname
MODULES += builtin_pwd
MODULES += builtin_search
MODULES += builtin_set
MODULES += builtin_shift
MODULES += builtin_source
MODULES += builtin_table
MODULES += builtin_test
MODULES += builtin_true
MODULES += builtin_unset

          
SOURCES = $(MODULES:%=%.c)
OBJECTS = $(MODULES:%=%.o)
DEPS    = $(MODULES:%=%.d)

# targets for this directory
# ---------------------------------------------------------------------------
all: Makefile $(ARCHIVE)
install: 

# dependencies
# ---------------------------------------------------------------------------
-include $(DEPS)

# local build configuration
# ---------------------------------------------------------------------------
INCLUDES += -I$(top_srcdir)/lib -I.
#INCLUDES += -I$(top_builddir)/lib
INCLUDES += -I$(top_srcdir)/src
#INCLUDES += -I$(top_builddir)/src
INCLUDES += -I$(top_builddir)

# include build.mk (makefile helper)
# ---------------------------------------------------------------------------
include $(top_builddir)/build.mk

