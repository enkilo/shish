# $Id: Makefile.in,v 1.6 2005/04/23 10:19:32 smoli Exp $
# ===========================================================================
#
# Makefile for src/fdtable subdirectory
#
# relative directories
# ---------------------------------------------------------------------------
srcdir       = @srcdir@
top_srcdir   = @top_srcdir@
top_builddir = @top_builddir@
thisdir      = src/fdtable/

VPATH        = $(srcdir)

# include global make configuration
# ---------------------------------------------------------------------------
include $(top_builddir)/config.mk
          
# configuration for this directory
# ---------------------------------------------------------------------------
ARCHIVE = fdtable.a
MODULES += fdtable_check
MODULES += fdtable_close
MODULES += fdtable_dump
MODULES += fdtable_dup
MODULES += fdtable_exec
MODULES += fdtable_gap
MODULES += fdtable_here
MODULES += fdtable_lazy
MODULES += fdtable_link
MODULES += fdtable_newfd
MODULES += fdtable_open
MODULES += fdtable_resolve
MODULES += fdtable_table
MODULES += fdtable_track
MODULES += fdtable_unexpected
MODULES += fdtable_unlink
MODULES += fdtable_up
MODULES += fdtable_wish

          
SOURCES = $(MODULES:%=%.c)
OBJECTS = $(MODULES:%=%.o)
DEPS    = $(MODULES:%=%.d)

# targets for this directory
# ---------------------------------------------------------------------------
all: Makefile $(ARCHIVE)
install: 

# dependencies
# ---------------------------------------------------------------------------
-include $(DEPS)

# local build configuration
# ---------------------------------------------------------------------------
INCLUDES += -I$(top_srcdir)/lib -I.
#INCLUDES += -I$(top_builddir)/lib
INCLUDES += -I$(top_srcdir)/src
#INCLUDES += -I$(top_builddir)/src
INCLUDES += -I$(top_builddir)
          
# include build.mk (makefile helper)
# ---------------------------------------------------------------------------
include $(top_builddir)/build.mk
