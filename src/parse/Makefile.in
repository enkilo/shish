# $Id: Makefile.in,v 1.6 2005/04/23 10:19:32 smoli Exp $
# ===========================================================================
#
# Makefile for src/parse subdirectory
#
# relative directories
# ---------------------------------------------------------------------------
srcdir       = @srcdir@
top_srcdir   = @top_srcdir@
top_builddir = @top_builddir@
thisdir      = src/parse/

VPATH        = $(srcdir)

# include global make configuration
# ---------------------------------------------------------------------------
include $(top_builddir)/config.mk
          
# configuration for this directory
# ---------------------------------------------------------------------------
ARCHIVE = parse.a
MODULES += parse_and_or
MODULES += parse_arith
MODULES += parse_bquoted
MODULES += parse_case
MODULES += parse_chartable
MODULES += parse_command
MODULES += parse_compound_list
MODULES += parse_dquoted
MODULES += parse_error
MODULES += parse_expect
MODULES += parse_for
MODULES += parse_function
MODULES += parse_getarg
MODULES += parse_gettok
MODULES += parse_grouping
MODULES += parse_here
MODULES += parse_if
MODULES += parse_init
MODULES += parse_keyword
MODULES += parse_list
MODULES += parse_loop
@DEBUG_FALSE@MODULES += parse_newnode
@DEBUG_TRUE@MODULES += parse_newnodedebug
MODULES += parse_param
MODULES += parse_pipeline
MODULES += parse_simple_command
MODULES += parse_simpletok
MODULES += parse_skipspace
MODULES += parse_squoted
MODULES += parse_string
MODULES += parse_subst
MODULES += parse_tokens
MODULES += parse_tokname
MODULES += parse_unquoted
MODULES += parse_validname
MODULES += parse_word

          
SOURCES = $(MODULES:%=%.c)
OBJECTS = $(MODULES:%=%.o)
DEPS    = $(MODULES:%=%.d)

# targets for this directory
# ---------------------------------------------------------------------------
all: Makefile $(ARCHIVE)
install: 

# dependencies
# ---------------------------------------------------------------------------
-include $(DEPS)

# local build configuration
# ---------------------------------------------------------------------------
INCLUDES += -I$(top_srcdir)/lib -I.
#INCLUDES += -I$(top_builddir)/lib
INCLUDES += -I$(top_srcdir)/src
#INCLUDES += -I$(top_builddir)/src
INCLUDES += -I$(top_builddir)
          
# include build.mk (makefile helper)
# ---------------------------------------------------------------------------
include $(top_builddir)/build.mk
