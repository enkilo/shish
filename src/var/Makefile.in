# $Id: Makefile.in,v 1.7 2005/04/30 18:16:48 smoli Exp $
# ===========================================================================
#
# Makefile for src/var subdirectory
#
# relative directories
# ---------------------------------------------------------------------------
srcdir       = @srcdir@
top_srcdir   = @top_srcdir@
top_builddir = @top_builddir@
thisdir      = src/var/

VPATH        = $(srcdir)

# include global make configuration
# ---------------------------------------------------------------------------
include $(top_builddir)/config.mk
          
# configuration for this directory
# ---------------------------------------------------------------------------
ARCHIVE = var.a
MODULES += var_bsearch
MODULES += var_chflg
MODULES += var_cleanup
MODULES += var_copys
MODULES += var_count
MODULES += var_create
MODULES += var_dump
MODULES += var_export
MODULES += var_get
MODULES += var_hsearch
MODULES += var_import
MODULES += var_init
MODULES += var_len
MODULES += var_lexhash
MODULES += var_print
MODULES += var_rndhash
MODULES += var_search
MODULES += var_set
MODULES += var_setsa
MODULES += var_setvint
MODULES += var_setvsa
MODULES += var_unset
MODULES += var_valid
MODULES += var_value
MODULES += var_vdefault
MODULES += var_vlen

          
SOURCES = $(MODULES:%=%.c)
OBJECTS = $(MODULES:%=%.o)
DEPS    = $(MODULES:%=%.d)

# targets for this directory
# ---------------------------------------------------------------------------
all: Makefile $(ARCHIVE)
install: 

# dependencies
# ---------------------------------------------------------------------------
-include $(DEPS)

# local build configuration
# ---------------------------------------------------------------------------
INCLUDES += -I$(top_srcdir)/lib -I.
#INCLUDES += -I$(top_builddir)/lib
INCLUDES += -I$(top_srcdir)/src
#INCLUDES += -I$(top_builddir)/src
INCLUDES += -I$(top_builddir)
INCLUDES += -include assert.h
          
# include build.mk (makefile helper)
# ---------------------------------------------------------------------------
include $(top_builddir)/build.mk
